<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Name</th>
      <th>Latitude</th>
      <th>Longitude</th>

    </tr>
  </thead>
  <tbody>
    {{#each category.placemarks}}
      <tr>
        <td>
          {{name}}
        </td>
        <td>
          {{latitude}}
        </td>
        <td>
          {{longitude}}
        </td>

        <td>
     <a href="/placemark/{{_id}}" class="ui icon button">
        <i class="fas fa-folder-open"></i>
    </a>
          <a href="/category/{{../category._id}}/deleteplacemark/{{_id}}" class="ui icon button">
            <i class="fas fa-trash"></i>
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
